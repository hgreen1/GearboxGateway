<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="carcoach.ipro.gearboxgateway" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HubActivity"
            android:label="@string/title_activity_hub" >
        </activity>
        <activity
            android:name=".DiagLoadActivity"
            android:label="@string/title_activity_diag_load" >
        </activity>
        <activity
            android:name=".CarStatActivity"
            android:label="@string/title_activity_car_stat" >
        </activity>
        <activity
            android:name=".JournalActivity"
            android:label="@string/title_activity_journal" >
        </activity>
        <activity
            android:name=".BTListActivity"
            android:label="@string/title_activity_btlist" >
        </activity>
        <activity
            android:name=".BTCarInfoActivity"
            android:label="@string/title_activity_btcar_info" >
        </activity>
    </application>

</manifest>
